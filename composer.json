{
    "name": "innmind/time-continuum",
    "type": "library",
    "description": "Library to manipulate time",
    "keywords": ["time"],
    "homepage": "http://github.com/Innmind/TimeContinuum",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "langlade.baptiste@gmail.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/TimeContinuum/issues"
    },
    "require": {
        "php": "~8.0",
        "psr/log": "^1.1",
        "innmind/immutable": "dev-next"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\TimeContinuum\\": "src/",
            "Fixtures\\Innmind\\TimeContinuum\\": "fixtures/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\TimeContinuum\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "vimeo/psalm": "~4.1",
        "innmind/black-box": "^4.0",
        "innmind/coding-standard": "^1.0"
    },
    "conflict": {
        "innmind/black-box": "<4.0|~5.0"
    },
    "suggest": {
        "innmind/black-box": "For property based testing"
    },
    "provide": {
        "innmind/black-box-sets": "4.0"
    }
}
